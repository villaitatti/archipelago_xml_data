<?xml version="1.0" encoding="UTF-8"?>
<generator_policy>

	<!-- Type -->
	
	<generator name="000-type" prefix="arc">
		<pattern>type/{type_id}</pattern>
	</generator>

	<generator name="010-type-aat" prefix="aat">
		<pattern>{aat}</pattern>
	</generator>

	<!-- Built Work-->

	<generator name="100-built_work" prefix="arc">
		<pattern>built_work/{bw_id}</pattern>
	</generator>

	<generator name="110-built_work-identifier" prefix="arc">
		<pattern>built_work/{bw_id}/identifier</pattern>
	</generator>

	<generator name="111-built_work-name" prefix="arc">
		<pattern>built_work/{bw_id}/name</pattern>
	</generator>

	<generator name="120-built_work-construction" prefix="arc">
		<pattern>built_work/{bw_id}/construction</pattern>
	</generator>

	<generator name="123-built_work-construction-date" prefix="arc">
		<pattern>built_work/{bw_id}/construction/date</pattern>
	</generator>

	<generator name="125-built_work-role" prefix="arc">
		<pattern>built_work/{bw_id}/role_{role_name}/{role_id}</pattern>
	</generator>

	<generator name="130-built_work-destruction" prefix="arc">
		<pattern>built_work/{bw_id}/destruction</pattern>
	</generator>

	<generator name="140-built_work-function" prefix="arc">
		<pattern>built_work/{bw_id}/function/{func_id}</pattern>
	</generator>

	<generator name="150-built_work-use" prefix="arc">
		<pattern>built_work/{bw_id}/use/{use_id}</pattern>
	</generator>

	<generator name="154-built_work-use-name" prefix="arc">
		<pattern>built_work/{bw_id}/use/{use_id}/name</pattern>
	</generator>

	<generator name="153-built_work-use-date" prefix="arc">
		<pattern>built_work/{bw_id}/use/{use_id}/date</pattern>
	</generator>

	<generator name="160-built_work-typology" prefix="arc">
		<pattern>built_work/{bw_id}/typology/{typology_id}</pattern>
	</generator>

	<generator name="163-built_work-typology-date" prefix="arc">
		<pattern>built_work/{bw_id}/typology/{typology_id}/date</pattern>
	</generator>

	<generator name="164-built_work-typology-name" prefix="arc">
		<pattern>built_work/{bw_id}/typology/{typology_id}/name</pattern>
	</generator>

	<generator name="170-built_work-measurement" prefix="arc">
		<pattern>built_work/{bw_id}/measurement/{m_id}</pattern>
	</generator>

	<generator name="171-built_work-measurement-dimension" prefix="arc">
		<pattern>built_work/{bw_id}/measurement/{m_id}/dimension</pattern>
	</generator>

	<generator name="172-built_work-measurement-unit" prefix="arc">
		<pattern>/measurement_unit/{unit}</pattern>
	</generator>


	<generator name="180-built_work-material" prefix="arc">
		<pattern>built_work/{bw_id}/material/{m_id}</pattern>
	</generator>

	<generator name="190-built_work-acquisition" prefix="arc">
		<pattern>built_work/{bw_id}/acquisition/{acquisition_id}</pattern>
	</generator>

	<generator name="193-built_work-acquisition-date" prefix="arc">
		<pattern>built_work/{bw_id}/acquisition/{acquisition_id}/date</pattern>
	</generator>

	<generator name="1A0-built_work-transfer_custody" prefix="arc">
		<pattern>built_work/{bw_id}/transfer_custody/{tf_id}</pattern>
	</generator>

	<generator name="1A3-built_work-transfer_custody-date" prefix="arc">
		<pattern>built_work/{bw_id}/transfer_custody/{tf_id}/date</pattern>
	</generator>


	<!-- Event -->
	<generator name="200-event" prefix="arc">
		<pattern>event/{event_id}</pattern>
	</generator>
	<generator name="210-event-identifier" prefix="arc">
		<pattern>event/{event_id}/identifier</pattern>
	</generator>
	<generator name="211-event-identifier-name" prefix="arc">
		<pattern>event/{event_id}/name</pattern>
	</generator>
	<generator name="220-event-typology" prefix="arc">
		<pattern>event/{event_id}/typology</pattern>
	</generator>
	<generator name="230-event-subtypology" prefix="arc">
		<pattern>event/{event_id}/subtypology</pattern>
	</generator>
	<generator name="240-event-island" prefix="arc">
		<pattern>event/{event_id}/island</pattern>
	</generator>
	<generator name="250-event-synopsis" prefix="arc">
		<pattern>event/{event_id}/synopsis</pattern>
	</generator>
	<generator name="263-event-date" prefix="arc">
		<pattern>event/{event_id}/date</pattern>
	</generator>
	<generator name="event/{event_id}/century" prefix="arc">
		<pattern>event/{event_id}/century</pattern>
	</generator>
	<generator name="event/{event_id}/century_fraction" prefix="arc">
		<pattern>event/{event_id}/century_fraction</pattern>
	</generator>
	<generator name="290-event-image" prefix="arc">
		<pattern>event/{event_id}/image</pattern>
	</generator>

	<generator name="291-event-source-event" prefix="arc">
		<pattern>event/{event_id}/source/{source_id}/event</pattern>
	</generator>


	<!-- Actor -->
	<generator name="300-actor" prefix="arc">
		<pattern>actor/{actor_id}</pattern>
	</generator>

	<!-- identify actors -->
	<generator name="310-actor-identifier" prefix="arc">
		<pattern>actor/{actor_id}/identifier</pattern>
	</generator>
	<generator name="311-actor-identifier-name" prefix="arc">
		<pattern>actor/{actor_id}/given_name</pattern>
	</generator>
	<generator name="312-actor-identifier-surname" prefix="arc">
		<pattern>actor/{actor_id}/surname</pattern>
	</generator>
	<generator name="313-actor-identifier-alias" prefix="arc">
		<pattern>actor/{actor_id}/alias</pattern>
	</generator>

	<generator name="314-actor-identifier-title" prefix="aat">
		<pattern>{aat}</pattern>
	</generator>

	<!-- Actor's birth -->
	<generator name="320-actor-birth" prefix="arc">
		<pattern>actor/{actor_id}/birth</pattern>
	</generator>
	<generator name="323-actor-birth-date" prefix="arc">
		<pattern>actor/{actor_id}/birth/date</pattern>
	</generator>

	<!-- Actor's death -->
	<generator name="330-actor-death" prefix="arc">
		<pattern>actor/{actor_id}/death</pattern>
	</generator>
	<generator name="333-actor-death-date" prefix="arc">
		<pattern>actor/{actor_id}/death/date</pattern>
	</generator>

	<!-- Actor's activity -->
	<generator name="340-actor-activity" prefix="arc">
		<pattern>actor/{actor_id}/activity/{activity_id}</pattern>
	</generator>
	<generator name="343-actor-activity-date" prefix="arc">
		<pattern>actor/{actor_id}/activity/{activity_id}/date</pattern>
	</generator>
	<generator name="349-actor-activity-OOC" prefix="arc">
		<pattern>actor/{actor_id}/activity/{activity_id}</pattern>
	</generator>	

	<!-- Actor's marriage -->
	<generator name="350-actor-marriage" prefix="arc">
		<pattern>actor/{actor_id}/marriage/</pattern>
	</generator>
	<generator name="353-actor-marriage-date" prefix="arc">
		<pattern>actor/{actor_id}marriage/date</pattern>
	</generator>

	<!-- Actor's will -->
	<generator name="360-actor-will" prefix="arc">
		<pattern>actor/{actor_id}will</pattern>
	</generator>
	<generator name="363-actor-will-date" prefix="arc">
		<pattern>actor/{actor_id}/will/date</pattern>
	</generator>

	<!-- Actor's biography -->
	<generator name="370-actor-biography" prefix="arc">
		<pattern>actor/{actor_id}/biography</pattern>
	</generator>

	<!-- Actor's image -->
	<generator name="380-actor-image" prefix="arc">
		<pattern>actor/{actor_id}image</pattern>
	</generator>

	<generator name="391-actor-source" prefix="arc">
		<pattern>actor/{actor_id}/source/{source_id}</pattern>
	</generator>
	<generator name="392-actor-source-name" prefix="arc">
		<pattern>actor/{actor_id}/source/{source_id}/name</pattern>
	</generator>
	<generator name="393-actor-source-surname" prefix="arc">
		<pattern>actor/{actor_id}/source/{source_id}/name</pattern>
	</generator>

	<generator name="394-actor-event" prefix="arc">
		<pattern>actor/{actor_id}/event/{event_id}/</pattern>
	</generator>
	<generator name="394-actor-event-role" prefix="arc">
		<pattern>actor/{actor_id}/event/{event_id}/role</pattern>
	</generator>



	<!-- Source -->
	<generator name="400-source" prefix="arc">
